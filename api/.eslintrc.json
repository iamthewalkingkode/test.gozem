{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": [ "prettier"],
    "rules": {
        "prettier/prettier": ["error"],
        "no-param-reassign": 0,
        "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
    },
    "parserOptions": {
        "ecmaVersion": 2020
    }
}
